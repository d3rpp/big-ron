{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "newline-before-return": "off",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "class-methods-use-this": "off",
        "no-console": "off",
        "linebreak-style": "error",
        "quotes": "off",
        "no-undef": "off"
    }
}
